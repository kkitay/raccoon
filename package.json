{
  "name": "raccoon",
  "productName": "Raccoon",
  "description": "Raccoon",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Kat Kitay",
    "email": "katkitay@gmail.com",
    "url": "https://kitay.co"
  },
  "main": "electron/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.kkitay.raccoon",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/assets/raccoon.png"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "copyfiles": "^2.1.0",
    "electromon": "^1.0.10",
    "electron-is-dev": "^1.0.1",
    "electron-settings": "^3.2.0",
    "lodash": "^4.17.11",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-typist": "^2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electromon .",
    "dev": "nf start",
    "preelectron-pack": "npm run build && copyfiles -u 1 electron/** electron/**/** build",
    "electron-pack": "electron-builder",
    "postelectron-pack": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.6",
    "electron-builder": "^20.31.2",
    "electron-packager": "^12.2.0",
    "foreman": "^3.0.1"
  }
}
